{
  "name": "websketch-extension",
  "version": "0.1.0",
  "description": "Chrome extension to capture web pages as WebSketch IR",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node build.js",
    "dev": "node build.js --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run typecheck && npm run lint && npm run test:run && npm run build"
  },
  "dependencies": {
    "@mcptoolshop/websketch-ir": "^0.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "esbuild": "^0.27.3",
    "eslint": "^8.56.0",
    "jsdom": "^28.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-tool-shop-org/websketch-extension.git"
  },
  "bugs": {
    "url": "https://github.com/mcp-tool-shop-org/websketch-extension/issues"
  },
  "keywords": [
    "chrome-extension",
    "websketch",
    "ui-capture",
    "web-scraping"
  ],
  "homepage": "https://github.com/mcp-tool-shop-org/websketch-extension#readme"
}
